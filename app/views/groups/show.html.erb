<h1><%= @group.name %> </h1>
<table>
  <tr>
    <th></th>
    <h4>USERS :</h4>
    <th></th>
  </tr>
<% @group.users.each do |user| -%>
  <tr class="<%= cycle('even','odd') %>">
    <td><%= image_tag('user.png') %></td>
    <td class="user_name"><%= user.name %></td>
    <td class="user_email"><%= user.email %></td>
</tr>
<% end -%>
</table>